#include <iostream>
#include <string>
#include <ctime>
#include <cstdlib>
#include <vector>

using namespace std;

#define inf 500

const int SIZE1 = 15;
const int RealSIZE = 11;
int board1[SIZE1][SIZE1] = { 0 };//本方1，对方-1，空白0  // [A][1]

struct xgznode
{
	int i;
	int j;
	int distance;
};

bool  operator > (xgznode a, xgznode b)
{
	if (a.distance > b.distance)
		return true;
	else
		return false;
}
